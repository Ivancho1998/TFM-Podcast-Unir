l_type":"code","source":["# Función para extraer temas y sentimiento de un título usando prompts\n","def extraer_temas_y_sentimiento(titulo):\n","    prompt = (\n","        f\"Analiza el título siguiente y devuelve los 5 temas principales \"\n","        f\"'temas: [lista de temas]'\"\n","        f\"Título: '{titulo}'\"\n","    )\n","\n","    # Generar respuesta del modelo\n","    response = pipe(prompt, max_new_tokens=1000, pad_token_id=tokenizer.eos_token_id, temperature=0.7)\n","\n","    # Procesar el resultado\n","    resultado = response[0][\"generated_text\"]\n","\n","    # Extraer solo el contenido que sigue al prompt\n","    resultado = resultado[len(prompt):].strip()\n","    return resultado\n","\n","# Crear un DataFrame con los resultados\n","data = []\n","\n","for titulo in titulos:\n","    resultado = extraer_temas_y_sentimiento(titulo)\n","    try:\n","        # temas, sentimiento = resultado.split(\", sentimiento: \")\n","        temas = temas.replace(\"temas: \", \"\").strip(\"[]\")  # Limpiar la lista de temas\n","        data.append({\"Título\": titulo, \"Temas\": temas})#, \"Sentimiento\": sentimiento.strip()})\n","    except ValueError:\n","        # Manejo de errores si el formato de la respuesta no es válido\n","        data.append({\"Título\": titulo, \"Temas\": \"Error en el formato\"})#, \"Sentimiento\": \"No definido\"})\n","\n","df = pd.DataFrame(data)\n","\n"],"metadata":{"id":"0tvJxUX2U-eT","executionInfo":{"status":"ok","timestamp":1736721824159,"user_tz":300,"elapsed":189107,"user":{"displayName":"ivan andres betancourt","userId":"14990897937864115566"}}},"execution_count":9,"outputs":[]},{"cell_type":"code","source":["df"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":175},"id":"z9tSw-HhVBmw","executionInfo":{"status":"ok","timestamp":1736721824161,"user_tz":300,"elapsed":20,"user":{"displayName":"ivan andres betancourt","userId":"14990897937864115566"}},"outputId":"3856f73a-70c0-45eb-b7e0-83bc91c439f4"},"execution_